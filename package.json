{
  "name": "baseline",
  "module": "server/serve.ts ",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "bun run --watch --inspect serve.ts",
    "start": "bun serve.ts",
    "tsc": "tsc"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false
  },
  "config": {
    "app_site_name": "Baseline",
    "dir_content": "./src/blocks",
    "dir_pages": "src/pages",
    "dir_public": "dist",
    "format_date_day": "numeric",
    "format_date_month": "long",
    "format_date_year": "numeric",
    "server_port": "2000",
    "server_router_style": "nextjs"
  },
  "bun": {
    "version": ">=1.1.37"
  },
  "peerDependencies": {
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@gotpop-platform/bun-plugin-copy-assets": "^1.0.9",
    "@gotpop-platform/package-components": "^1.0.25",
    "@gotpop-platform/package-icons": "^1.0.0",
    "@gotpop-platform/package-jsx-factory": "^1.0.9",
    "@gotpop-platform/package-logger": "^1.0.12",
    "@gotpop-platform/package-markdown": "^1.0.25",
    "@gotpop-platform/package-server": "1.0.95",
    "@gotpop-platform/package-utilities": "^1.0.18"
  }
}
